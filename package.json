    "react",
    "svg",
    "mobile-app"
  ],
  "author": "Hasane",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.0.0",
    "pwa-asset-generator": "^6.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
