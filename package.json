{
  "name": "kat-plani-olusturucu",
  "version": "1.0.0",
  "description": "Dinamik mimari kat planı oluşturma PWA uygulaması",
  "main": "index.html",
  "scripts": {
    "start": "npx serve -s . -l 3000",
    "build": "echo 'No build process needed - static files'",
